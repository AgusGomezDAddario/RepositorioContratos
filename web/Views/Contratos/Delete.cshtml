@model web.Models.Contrato

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar Contrato</h1>

<h3>¿Está seguro que desea eliminar el contrato de número @Model.Numero?</h3>
<div>
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> 
        <input type="submit" value="Cancelar" class="btn btn-primary" style="color:white" />
    </form>
</div>